{
    "name": "codingmonkeys/file-flux",
    "description": "Convert your audio",
    "keywords": [
        "codingmonkeys",
        "file-flux"
    ],
    "homepage": "https://github.com/codingmonkeys/file-flux",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Michael",
            "email": "michael@codingmonkeys.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Codingmonkeys\\FileFlux\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codingmonkeys\\FileFlux\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codingmonkeys\\FileFlux\\FileFluxServiceProvider"
            ],
            "aliases": {
                "FileFlux": "Codingmonkeys\\FileFlux\\FileFluxFacade"
            }
        }
    }
}
